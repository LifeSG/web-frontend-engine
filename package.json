	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run build && npm run test"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"npm run lint"
		]
	},
	"overrides": {
		"trim-newlines@<3.0.1": "3.0.1",
		"trim-newlines@4.0.0": "4.0.1",
		"trim@<0.0.3": "0.0.3",
		"glob-parent@<5.1.2": "5.1.2",
		"nth-check@<2.0.1": "2.0.1",
